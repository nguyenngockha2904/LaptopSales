<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="88.0" prefWidth="642.0" styleClass="bg5" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.superducks.laptopsales.controllers.EditWarehouse">
   <children>
      <TextField fx:id="txtProduct" editable="false" focusTraversable="false" layoutX="76.0" layoutY="32.0" prefHeight="25.0" prefWidth="303.0" style="-fx-alignment: CENTER;" />
      <Label layoutX="14.0" layoutY="34.0" text="Product" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="txtRemaining" layoutX="482.0" layoutY="32.0" onKeyReleased="#text_Changed" prefHeight="25.0" prefWidth="69.0" style="-fx-alignment: CENTER;" />
      <Label layoutX="401.0" layoutY="34.0" text="Available" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ImageView fx:id="btnNonAccept" fitHeight="40.0" fitWidth="40.0" layoutX="574.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../icons/web_ui_brown/checkmark.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnAccept" fitHeight="40.0" fitWidth="40.0" layoutX="574.0" layoutY="25.0" onMouseClicked="#btnAccept_Clicked" pickOnBounds="true" preserveRatio="true" visible="false">
         <image>
            <Image url="@../icons/web_ui_color/checkmark.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
